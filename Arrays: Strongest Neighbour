void maximumAdjacent(int sizeOfArray, int arr[]){
    vector <int> v;
    for(int i=0;i<sizeOfArray;i++)
    v.push_back(arr[i]);
    // sort(v.begin(),v.end());
    for(int i=0;i<sizeOfArray-1;i++)
    {
        if(v[i]<=v[i+1])
        {
            cout<<v[i+1]<<" ";
        }
        else
        {
            cout<<v[i]<<" ";
        }
    }
   
}
